{
    "name": "league/uri-manipulations",
    "type": "library",
    "description" : "URI manipulation library",
    "keywords": [
        "url",
        "uri",
        "rfc3986",
        "psr-7",
        "Manipulations",
        "manipulation",
        "modifiers",
        "middlewares",
        "formatter",
        "references"
    ],
    "license": "MIT",
    "homepage": "http://url.thephpleague.com",
    "authors": [
        {
            "name" : "Ignace Nyamagana Butera",
            "email" : "nyamsprod@gmail.com",
            "homepage" : "https://nyamsprod.com"
        }
    ],
    "support": {
        "forum": "https://groups.google.com/forum/#!forum/thephpleague",
        "issues": "https://github.com/thephpleague/uri/issues"
    },
    "require": {
        "php" : ">=5.6",
        "psr/http-message": "^1.0",
        "league/uri-components": "dev-master",
        "league/uri-interfaces": "~0.2"
    },
    "require-dev": {
        "league/uri-schemes": "dev-master",
        "guzzlehttp/psr7": "^1.2",
        "league/uri-parser": "~0.2",
        "friendsofphp/php-cs-fixer": "^1.9",
        "phpunit/phpunit" : "^5.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Uri\\Modifiers\\": "src"
        },
        "files": ["src/functions_include.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "LeagueTest\\Uri\\Modifiers\\": "test"
        }
    },
    "suggest": {
        "league/uri-parser": "Allow parsing any URI using RFC3986/RFC3987 compliant algorithm",
        "league/uri-schemes": "Allow manipulating URI objects"
    },
    "scripts": {
        "test": "phpunit --coverage-text; php-cs-fixer fix -v --diff --dry-run;",
        "phpunit": "phpunit --coverage-text",
        "phpcs": "php-cs-fixer fix -v --diff --dry-run;"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
